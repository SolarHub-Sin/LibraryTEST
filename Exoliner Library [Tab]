	local function Dragify()
		local script = Instance.new('LocalScript', Background)

		-- Decompiled with the Synapse X Luau decompiler.

		local l__UserInputService__1 = game:GetService("UserInputService");
		local function dragify(p1)
			local dragToggle = nil;
			local dragInput = nil;
			local dragStart = nil;
			local startPos
			local function updateInput(p2)
				local v1 = p2.Position - dragStart;
				game:GetService("TweenService"):Create(p1, TweenInfo.new(0.25), {
					Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + v1.X, startPos.Y.Scale, startPos.Y.Offset + v1.Y)
				}):Play();
			end;
			p1.InputBegan:Connect(function(p3)
				if p3.UserInputType ~= Enum.UserInputType.MouseButton1 then
					if p3.UserInputType == Enum.UserInputType.Touch then
						if l__UserInputService__1:GetFocusedTextBox() == nil then
							dragToggle = true;
							dragStart = p3.Position;
							startPos = p1.Position;
							p3.Changed:Connect(function()
								if p3.UserInputState == Enum.UserInputState.End then
									dragToggle = false;
								end;
							end);
						end;
					end;
				elseif l__UserInputService__1:GetFocusedTextBox() == nil then
					dragToggle = true;
					dragStart = p3.Position;
					startPos = p1.Position;
					p3.Changed:Connect(function()
						if p3.UserInputState == Enum.UserInputState.End then
							dragToggle = false;
						end;
					end);
				end;
			end);
			p1.InputChanged:Connect(function(p4)
				if p4.UserInputType ~= Enum.UserInputType.MouseMovement then
					if p4.UserInputType == Enum.UserInputType.Touch then
						dragInput = p4;
					end;
				else
					dragInput = p4;
				end;
			end);
			game:GetService("UserInputService").InputChanged:Connect(function(p5)
				if p5 == dragInput then
					if dragToggle then
						updateInput(p5);
					end;
				end;
			end);
		end;
		dragify(script.Parent);

	end

	local function Visible()
		local script = Instance.new('LocalScript', Background)

		for i,v in pairs(script.Parent:GetChildren()) do
			if v:IsA('Frame') then
				v.Visible = false
			end
		end
	end

	Visible()
	Dragify()
	
	local Tab = {}
	
	function Tab.Tab(Text)
		local home = Instance.new("ScrollingFrame")
		local UIListLayout = Instance.new("UIListLayout")
		local home_2 = Instance.new("TextButton")
		local Icon = Instance.new("ImageLabel")
		local Namet = Instance.new("TextLabel")
		home.Name = "home"
		home.Parent = Background
		home.Active = true
		home.BackgroundTransparency = 1.000
		home.BorderSizePixel = 0
		home.Position = UDim2.new(0, 134, 0, 46)
		home.Size = UDim2.new(1, -142, 1, -56)
		home.CanvasSize = UDim2.new(0, 0, 0, 250)
		home.ScrollBarThickness = 3
		UIListLayout.Parent = home
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.Padding = UDim.new(0, 10)
		home_2.Name = "home"
		home_2.Parent = Pages_Container
		home_2.BackgroundTransparency = 1.000
		home_2.BorderSizePixel = 0
		home_2.Size = UDim2.new(1, 0, -0.00510789594, 26)
		home_2.ZIndex = 3
		home_2.AutoButtonColor = false
		home_2.Font = Enum.Font.Gotham
		home_2.Text = ""
		home_2.TextColor3 = Color3.fromRGB(160, 68, 255)
		home_2.TextSize = 14.000
		home_2.TextXAlignment = Enum.TextXAlignment.Right
		Icon.Name = "Icon"
		Icon.Parent = home_2
		Icon.AnchorPoint = Vector2.new(0, 0.5)
		Icon.BackgroundTransparency = 1.000
		Icon.Position = UDim2.new(0, 12, 0.5, 0)
		Icon.Size = UDim2.new(0, 18, 0, 18)
		Icon.ZIndex = 3
		Icon.Image = "rbxassetid://4370345144"
		Icon.ImageColor3 = Color3.fromRGB(160, 68, 255)
		Namet.Name = "Namet"
		Namet.Parent = home_2
		Namet.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Namet.BackgroundTransparency = 1.000
		Namet.Position = UDim2.new(0.301587313, 0, 0, 0)
		Namet.Size = UDim2.new(0.698412716, 0, -0.0049999929, 26)
		Namet.Font = Enum.Font.Gotham
		Namet.Text = "Home"
		Namet.TextColor3 = Color3.fromRGB(160, 68, 255)
		Namet.TextSize = 14.000
		Namet.TextXAlignment = Enum.TextXAlignment.Left

		local function Open()
			local script = Instance.new('LocalScript', home_2)

			local function Tween(Point_A, Time, Point_B)
				game:GetService("TweenService"):Create(Point_A, TweenInfo(Time, Enum.EasingStyle.Cubic, Enum.EasingDirection.InOut), Point_B):Play()
			end

			script.Parent.MouseButton1Click:Connect(function()
				local NAME = script.Parent.Name
				Tween(script.Parent, .25, {TextTransparency = 0})
				Tween(script.Parent.Icon, .25, {ImageTransparency = 0})
				for i,v in pairs(script.Parent.Parent:GetChildren()) do
					if v:IsA("TextButton") then
						if v.Name ~= NAME then
							Tween(v, .25, {TextTransparency = .5})
							Tween(v.Icon, .25, {ImageTransparency = .5})
						end
					end
				end
				script.Parent.Parent.Parent.Parent:FindFirstChild(NAME).Visible = true
				script.Parent.Parent.Parent.Parent:FindFirstChild(NAME):TweenSize(UDim2.new(1, -142, 1, -56), "InOut", "Linear", "0.25");
				for i,v in pairs(script.Parent.Parent.Parent.Parent:GetChildren()) do
					if v:IsA("ScrollingFrame") then
						if v.Name ~= NAME then
							v:TweenSize(UDim2.new(1, -142, 1, -428), "InOut", "Linear", "0.25");
						end
					end
				end
				wait(.3)
				for i,v in pairs(script.Parent.Parent.Parent.Parent:GetChildren()) do
					if v:IsA("ScrollingFrame") then
						if v.Name ~= NAME then
							v.Visible = false;
						end
					end
				end
			end)

		end

		Open()
	end
	
	return Tab
end

return Exoliner
